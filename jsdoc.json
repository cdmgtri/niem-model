{
  "source": {
    "exclude": [
      "src/typedefs"
    ]
  },
  "options": {
    "access": ["public", "protected", "undefined"]
  },
  "plugins": [
    "node_modules/jsdoc-tsimport-plugin/index.js",
    "node_modules/jsdoc-typeof-plugin",
    "plugins/markdown"
  ],
  "docdash": {
    "sort": true,
    "search": true
  },
  "templates": {
    "referenceTitle": "NIEM Model Classes",
    "disableSort": false,
    "collapse": true,
    "default": {
      "includeDate": false,
      "layoutFile": "./assets/docs/layout.tmpl",
      "staticFiles": {
        "include": [
          "./assets/docs"
        ]
      }
    }
  }
}